<odoo noupdate="1">
        
        <!-- Sales Order Sequences  -->

        <record id="cl_sequence_sale_normal" model="ir.sequence">
            <field name="name">NVNA Pedido Venta Normal (Secuencia)</field>
            <field name="prefix">NVNA/%(range_year)s/</field>
            <field eval="100000" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="1" name="company_id"/>
            <field name="padding">6</field>
        </record>

